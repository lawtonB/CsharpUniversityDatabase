<h1>To Do List</h1>
  <hr />
  <h2>Students:</h2>
  @if (Model.Count != 0)
  {
    <ul>
      @foreach (var Student in Model)
      {
        <li><a href='/Students/@Student.GetId()'>@Student.GetName() - @Student.GetEnrollmentDate</a> <a href="/Students/delete/@Student.GetId()">-Delete</a></li>
      }
    </ul>
  }
  <h4><a href="/Students/new">Add new Student</a></h4>
  <h4><a href="/">Return home</a></h4>
